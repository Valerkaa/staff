3e \u0434\u043d\u044f. \u0410\u0431\u043e \u043f\u0440\u043e\u0446\u0435\u0441\u0438 \u0432 \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u0457 \u043f\u043e\u0431\u0443\u0434\u043e\u0432\u0430\u043d\u0456 \u0442\u0430\u043a, \u0449\u043e \u0441\u043f\u0456\u0432\u0440\u043e\u0431\u0456\u0442\u043d\u0438\u043a \u0446\u0456\u043b\u0438\u0439 \u0434\u0435\u043d\u044c \u043d\u0456\u0447\u043e\u0433\u043e \u043d\u0435 \u0440\u043e\u0431\u0438\u0442\u044c, \u0430 \u0432\u0432\u0435\u0447\u0435\u0440\u0456 \u043d\u0430 \u043d\u044c\u043e\u0433\u043e \u0437\u0432\u0430\u043b\u044e\u044e\u0442\u044c\u0441\u044f \u0437\u0430\u0432\u0434\u0430\u043d\u043d\u044f \u0437 \u0443\u0441\u0456\u0445 \u0432\u0456\u0434\u0434\u0456\u043b\u0456\u0432. \u0426\u0435 \u043f\u0440\u0438\u0447\u0438\u043d\u0430, \u0449\u043e\u0431 \u0434\u043e\u043f\u043b\u0430\u0447\u0443\u0432\u0430\u0442\u0438 \u0437\u0430 \u043e\u0432\u0435\u0440\u0442\u0430\u0439\u043c.<\/p>\n\n\n\n<p>\u0412\u0430\u0436\u043b\u0438\u0432\u0430 \u0443\u043c\u043e\u0432\u0430 \u2014 \u0432\u0438 \u0441\u0430\u043c\u0456 \u043f\u043e\u0432\u0438\u043d\u043d\u0456 \u0432\u0456\u0440\u0438\u0442\u0438 \u0432 \u0442\u0435, \u0449\u043e \u0437\u0430\u0441\u043b\u0443\u0433\u043e\u0432\u0443\u0454\u0442\u0435 \u043e\u0442\u0440\u0438\u043c\u0443\u0432\u0430\u0442\u0438 \u0431\u0456\u043b\u044c\u0448\u0435. \u042f\u043a\u0449\u043e \u0432\u0438 \u043d\u0435 \u043c\u043e\u0436\u0435\u0442\u0435 \u043f\u0435\u0440\u0435\u043a\u043e\u043d\u0430\u0442\u0438 \u0441\u0435\u0431\u0435, \u043f\u0435\u0440\u0435\u043a\u043e\u043d\u0430\u0442\u0438 \u043a\u0435\u0440\u0456\u0432\u043d\u0438\u043a\u0430 \u0442\u043e\u0447\u043d\u043e \u043d\u0435 \u0432\u0434\u0430\u0441\u0442\u044c\u0441\u044f. \u0412\u0456\u0434\u0447\u0443\u0432\u0448\u0438 \u043d\u0435\u0432\u043f\u0435\u0432\u043d\u0435\u043d\u0456\u0441\u0442\u044c, \u0440\u043e\u0437\u0431\u0435\u0440\u0456\u0442\u044c\u0441\u044f, \u0443 \u0447\u043e\u043c\u0443 \u0457\u0457 \u043f\u0440\u0438\u0447\u0438\u043d\u0430. \u041c\u043e\u0436\u043b\u0438\u0432\u043e, \u0432\u0430\u043c \u0432\u0430\u0440\u0442\u043e \u0449\u0435 \u043f\u0456\u0432 \u0440\u043e\u043a\u0443 \u043f\u043e\u043f\u0440\u0430\u0446\u044e\u0432\u0430\u0442\u0438, \u043f\u0435\u0440\u0448 \u043d\u0456\u0436 \u0432\u0438 \u0437\u043c\u043e\u0436\u0435\u0442\u0435 \u0437\u0456\u0437\u043d\u0430\u0442\u0438\u0441\u044f \u0441\u043e\u0431\u0456, \u0449\u043e \u0442\u0435\u043f\u0435\u0440 \u0437\u0430\u0441\u043b\u0443\u0433\u043e\u0432\u0443\u0454\u0442\u0435 \u043d\u0430 \u0432\u0438\u0449\u0443 \u043a\u043e\u043c\u043f\u0435\u043d\u0441\u0430\u0446\u0456\u044e.<\/p>\n\n\n\n<p><strong>\u0429\u043e \u043f\u043e\u0447\u0438\u0442\u0430\u0442\u0438: <\/strong>\u0441\u0442\u0430\u0442\u0442\u044e \u0410\u0434\u0440\u0456\u0454\u043d\u043d \u0411\u043e\u043b\u0433\u0435\u0440 <a href=\"https:\/\/codewithoutrules.com\/2018\/08\/22\/engineer-a-raise\/\" target=\"_blank\" rel=\"noopener nofollow\"><em>&#8220;How to engineer a raise&#8221;<\/em><\/a>. \u0410\u0432\u0442\u043e\u0440\u043a\u0430 \u0443\u0441\u043f\u0456\u0448\u043d\u043e \u0437\u0430\u0441\u0442\u043e\u0441\u043e\u0432\u0443\u0432\u0430\u043b\u0430 \u0440\u0456\u0437\u043d\u0456 \u0441\u0442\u0440\u0430\u0442\u0435\u0433\u0456\u0457 \u043f\u0435\u0440\u0435\u043c\u043e\u0432\u0438\u043d, \u0456 \u0432 \u0431\u0456\u043b\u044c\u0448\u043e\u0441\u0442\u0456 \u0432\u0438\u043f\u0430\u0434\u043a\u0456\u0432 \u0457\u0439 \u0432\u0434\u0430\u043b\u043e\u0441\u044f \u0434\u043e\u043c\u043e\u0432\u0438\u0442\u0438\u0441\u044f \u043f\u0440\u043e \u043f\u0456\u0434\u0432\u0438\u0449\u0435\u043d\u043d\u044f \u0437\u043f \u043d\u0430 20\u201330%.<\/p>\n\n\n\n<p><\/p>\n\n\n\n<h2 class=\"has-large-font-size wp-block-heading\"><strong>\u00ab\u041f\u043b\u0430\u0442\u0456\u0442\u044c \u0431\u0456\u043b\u044c\u0448\u0435, \u0430\u0431\u043e \u044f \u0437\u0432\u0456\u043b\u044c\u043d\u044f\u044e\u0441\u044f\u00bb: \u0447\u043e\u043c\u0443 \u0448\u0430\u043d\u0442\u0430\u0436 \u0447\u0438 \u043a\u043e\u043d\u0442\u0440\u043e\u0444\u0435\u0440 \u2014 \u043f\u043e\u0433\u0430\u043d\u0430 \u0441\u0442\u0440\u0430\u0442\u0435\u0433\u0456\u044f<\/strong><\/h2>\n\n\n\n<p>\u0417\u0430\u0437\u0432\u0438\u0447\u0430\u0439 \u0441\u043f\u0456\u0432\u0440\u043e\u0431\u0456\u0442\u043d\u0438\u043a\u0438 \u043a\u0430\u0436\u0443\u0442\u044c \u00ab\u041f\u043b\u0430\u0442\u0456\u0442\u044c \u0431\u0456\u043b\u044c\u0448\u0435, \u0430\u0431\u043e \u044f \u0437\u0432\u0456\u043b\u044c\u043d\u044f\u044e\u0441\u044f\u00bb \u0443 \u0442\u043e\u043c\u0443 \u0432\u0438\u043f\u0430\u0434\u043a\u0443, \u043a\u043e\u043b\u0438 \u043d\u0435 \u0434\u0443\u0436\u0435 \u0442\u0440\u0438\u043c\u0430\u044e\u0442\u044c\u0441\u044f \u043f\u043e\u0442\u043e\u0447\u043d\u043e\u0433\u043e \u043c\u0456\u0441\u0446\u044f \u0440\u043e\u0431\u043e\u0442\u0438. \u0426\u0435 \u043b\u044e\u0434\u0438, \u044f\u043a\u0456 \u0432\u0436\u0435 \u043e\u0442\u0440\u0438\u043c\u0430\u043b\u0438 \u043a\u0440\u0430\u0449\u0443 \u043f\u0440\u043e\u043f\u043e\u0437\u0438\u0446\u0456\u044e \u0430\u0431\u043e \u043d\u0430\u0441\u0442\u0456\u043b\u044c\u043a\u0438 \u0432\u0438\u0433\u043e\u0440\u0456\u043b\u0438 \u0439 \u043d\u0435\u0437\u0430\u0434\u043e\u0432\u043e\u043b\u0435\u043d\u0456 \u0440\u043e\u0431\u043e\u0442\u043e\u044e \u0432 \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u0457 \u0447\u0438 \u043f\u0440\u043e\u0454\u043a\u0442\u043e\u043c, \u0449\u043e \u0432\u0438\u0440\u0456\u0448\u0438\u043b\u0438 \u043f\u0456\u0442\u0438 \u0432\u0430-\u0431\u0430\u043d\u043a.<\/p>\n\n\n\n<p>\u041d\u0430\u0432\u0456\u0442\u044c \u044f\u043a\u0449\u043e \u043f\u0456\u0441\u043b\u044f \u0442\u0430\u043a\u043e\u0433\u043e \u0430\u0440\u0433\u0443\u043c\u0435\u043d\u0442\u0443 \u043b\u044e\u0434\u0438\u043d\u0456 \u043f\u0456\u0434\u0432\u0438\u0449\u0430\u0442\u044c \u0437\u0430\u0440\u043f\u043b\u0430\u0442\u0443, \u0432\u043e\u043d\u0430, \u043d\u0430\u043f\u0435\u0432\u043d\u043e, \u043f\u0440\u043e\u043f\u0440\u0430\u0446\u044e\u0454 \u043d\u0435\u0434\u043e\u0432\u0433\u043e. \u0417\u0430\u0437\u0432\u0438\u0447\u0430\u0439 \u0440\u043e\u0431\u043e\u0442\u043e\u0434\u0430\u0432\u0446\u0456 \u0432\u0432\u0430\u0436\u0430\u044e\u0442\u044c, \u0449\u043e \u043f\u0456\u0434\u0432\u0438\u0449\u0435\u043d\u043d\u044f \u0437\u0430\u0440\u043f\u043b\u0430\u0442\u043d\u0456 \u2014 \u043a\u043e\u0440\u043e\u0442\u043a\u043e\u0441\u0442\u0440\u043e\u043a\u043e\u0432\u0430 \u043c\u043e\u0442\u0438\u0432\u0430\u0446\u0456\u044f, \u044f\u043a\u0430 \u043f\u0440\u0430\u0446\u044e\u0454 \u0431\u043b\u0438\u0437\u044c\u043a\u043e \u0442\u0440\u044c\u043e\u0445 \u043c\u0456\u0441\u044f\u0446\u0456\u0432. \u041f\u0456\u0434\u0432\u0438\u0449\u0443\u0432\u0430\u0442\u0438 \u0440\u0456\u0432\u0435\u043d\u044c \u043a\u043e\u043c\u043f\u0435\u043d\u0441\u0430\u0446\u0456\u0457 \u0442\u0430\u043a \u0447\u0430\u0441\u0442\u043e \u2014 \u043d\u0435\u0432\u0438\u0433\u0456\u0434\u043d\u043e \u0442\u0430 \u0431\u0435\u0437\u0437\u043c\u0456\u0441\u0442\u043e\u0432\u043d\u043e. \u041a\u0440\u0456\u043c \u0442\u043e\u0433\u043e, \u0437\u0433\u0456\u0434\u043d\u043e \u0437 <a href=\"https:\/\/www.phaidoninternational.com\/blog\/2020\/02\/why-you-should-reject-a-counter-offer\" target=\"_blank\" rel=\"noopener nofollow\">\u043c\u0456\u0436\u043d\u0430\u0440\u043e\u0434\u043d\u0438\u043c\u0438 \u0434\u043e\u0441\u043b\u0456\u0434\u0436\u0435\u043d\u043d\u044f\u043c\u0438<\/a>, 70\u201380% \u043b\u044e\u0434\u0435\u0439, \u044f\u043a\u0456 \u0437\u0430\u043b\u0438\u0448\u0438\u043b\u0438\u0441\u044f \u0432 \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u0457 \u043f\u0456\u0441\u043b\u044f \u043f\u0456\u0434\u0432\u0438\u0449\u0435\u043d\u043d\u044f \u0437\u043f \u0443 \u0437\u0432\u2019\u044f\u0437\u043a\u0443 \u0437 \u043a\u043e\u043d\u0442\u0440\u043e\u0444\u0444\u0435\u0440\u043e\u043c, \u0432\u043f\u0440\u043e\u0434\u043e\u0432\u0436 6 \u043c\u0456\u0441\u044f\u0446\u0456\u0432 \u0443\u0441\u0435 \u0436 \u0437\u0432\u0456\u043b\u044c\u043d\u044f\u044e\u0442\u044c\u0441\u044f.&nbsp;<\/p>\n\n\n\n<p>\u041e\u0442\u0436\u0435, \u044f\u043a\u0449\u043e \u0432\u0438 \u0441\u043a\u0430\u0436\u0435\u0442\u0435 \u043a\u0435\u0440\u0456\u0432\u043d\u0438\u043a\u0443, \u0449\u043e \u0443 \u0432\u0430\u0441 \u043d\u0430 \u0440\u0443\u043a\u0430\u0445 \u0454 \u0432\u0438\u0433\u0456\u0434\u043d\u0456\u0448\u0430 \u043f\u0440\u043e\u043f\u043e\u0437\u0438\u0446\u0456\u044f \u0432\u0456\u0434 \u0456\u043d\u0448\u043e\u0457 \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u0457 \u0430\u0431\u043e \u0432\u0438 \u043d\u0435 \u0433\u043e\u0442\u043e\u0432\u0456 \u043f\u0440\u043e\u0434\u043e\u0432\u0436\u0443\u0432\u0430\u0442\u0438 \u0440\u043e\u0431\u043e\u0442\u0443 \u0431\u0435\u0437 \u043f\u0456\u0434\u0432\u0438\u0449\u0435\u043d\u043d\u044f, \u0437 \u0432\u0430\u043c\u0438 \u043c\u043e\u0436\u0443\u0442\u044c \u043f\u043e\u043f\u0440\u043e\u0449\u0430\u0442\u0438\u0441\u044f. \u0412\u0438\u043d\u044f\u0442\u043e\u043a \u2014 \u0432\u0438 \u0446\u0456\u043d\u043d\u0438\u0439 \u0447\u0438 \u0432\u0443\u0437\u044c\u043a\u043e \u0441\u043f\u0435\u0446\u0456\u0430\u043b\u0456\u0437\u043e\u0432\u0430\u043d\u0438\u0439 \u0444\u0430\u0445\u0456\u0432\u0435\u0446\u044c, \u044f\u043a\u043e\u0433\u043e \u0431\u0443\u0434\u0435 \u0432\u0430\u0436\u043a\u043e \u0442\u0430 \u0434\u043e\u0440\u043e\u0433\u043e \u0437\u0430\u043c\u0456\u043d\u0438\u0442\u0438.<\/p>\n\n\n\n<p>\u041a\u0440\u0430\u0449\u0435 \u0441\u043a\u0430\u0437\u0430\u0442\u0438: <em>\u00ab\u0427\u0430\u0441 \u0432\u0456\u0434 \u0447\u0430\u0441\u0443 \u044f \u043c\u043e\u043d\u0456\u0442\u043e\u0440\u044e \u0440\u0438\u043d\u043e\u043a, \u0430 \u0442\u0430\u043a\u043e\u0436 \u0432\u0456\u0434\u043c\u043e\u0432\u043b\u044f\u044e\u0441\u044c \u0432\u0456\u0434 \u043f\u0440\u043e\u043f\u043e\u0437\u0438\u0446\u0456\u0439 \u0456\u043d\u0448\u0438\u0445 \u043a\u043e\u043c\u043f\u0430\