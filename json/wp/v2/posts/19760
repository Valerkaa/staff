agraph -->\n<p><em>\u00ab\u041f\u0440\u043e\u043f\u043e\u043d\u0443\u044e \u0440\u043e\u0437\u0433\u043b\u044f\u043d\u0443\u0442\u0438 \u043d\u0430 \u0446\u0438\u0444\u0440\u0430\u0445, \u0441\u043a\u0456\u043b\u044c\u043a\u0438 \u043b\u044e\u0434\u0438\u043d\u043e-\u0433\u043e\u0434\u0438\u043d \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u0457 \u043f\u043e\u0442\u0440\u0456\u0431\u043d\u043e \u0432\u0438\u0442\u0440\u0430\u0442\u0438\u0442\u0438 \u043d\u0430 \u043d\u0430\u0439\u043c \u043e\u0434\u043d\u043e\u0433\u043e \u043d\u043e\u0432\u043e\u0433\u043e \u0444\u0430\u0445\u0456\u0432\u0446\u044f. \u0417\u0430\u0437\u0432\u0438\u0447\u0430\u0439 \u0434\u043b\u044f \u0446\u044c\u043e\u0433\u043e \u043f\u043e\u0442\u0440\u0456\u0431\u043d\u043e:<\/em><\/p>\n<!-- \/wp:paragraph -->\n\n<!-- wp:paragraph -->\n<p><em>\u041f\u0440\u043e\u0432\u0435\u0441\u0442\u0438 <\/em><strong><em>2\u20133<\/em><\/strong><em> \u0441\u043f\u0456\u0432\u0431\u0435\u0441\u0456\u0434\u0438 (\u0430 \u0447\u0430\u0441\u0442\u043e \u0431\u0456\u043b\u044c\u0448\u0435) \u0442\u0440\u0438\u0432\u0430\u043b\u0456\u0441\u0442\u044e \u043f\u043e \u0433\u043e\u0434\u0438\u043d\u0456. \u041d\u0430 \u0446\u0438\u0445 \u0434\u0432\u043e\u0445 \u0441\u043f\u0456\u0432\u0431\u0435\u0441\u0456\u0434\u0430\u0445 \u0431\u0440\u0430\u0442\u0438\u043c\u0443\u0442\u044c \u0443\u0447\u0430\u0441\u0442\u044c <\/em><strong><em>2\u20133<\/em><\/strong><em> \u043b\u044e\u0434\u0438\u043d\u0438. \u0412\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u043d\u043e, \u0432\u0438\u0442\u0440\u0430\u0442\u0438 \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u0457 \u043d\u0430 \u043f\u0440\u043e\u0432\u0435\u0434\u0435\u043d\u043d\u044f \u043e\u0434\u043d\u043e\u0433\u043e \u043f\u043e\u0432\u043d\u043e\u0433\u043e \u043a\u043e\u043b\u0430 \u0441\u043f\u0456\u0432\u0431\u0435\u0441\u0456\u0434 \u2014 <\/em><strong><em>3<\/em><\/strong><em> \u043b\u044e\u0434\u0438\u043d\u0438 \u043f\u043e <\/em><strong><em>2\u20133 <\/em><\/strong><