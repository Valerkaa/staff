p>\n\n\n\n<p><strong>1. \u0412\u0438\u0437\u043d\u0430\u0447\u0442\u0435, \u0445\u0442\u043e \u0441\u0430\u043c\u0435 \u0432\u0430\u043c \u043f\u043e\u0442\u0440\u0456\u0431\u0435\u043d. <\/strong>\u0429\u043e \u043a\u0440\u0438\u0442\u0438\u0447\u043d\u043e \u0432\u0430\u0436\u043b\u0438\u0432\u043e \u0434\u043b\u044f \u0446\u044c\u043e\u0433\u043e \u043f\u0440\u043e\u0454\u043a\u0442\u0443, \u0430 \u0449\u043e \u043c\u0435\u043d\u0448 \u043f\u0440\u0456\u043e\u0440\u0438\u0442\u0435\u0442\u043d\u043e \u0456 \u043c\u043e\u0436\u043d\u0430 \u0432\u0438\u0432\u0447\u0438\u0442\u0438 \u0432 \u043f\u0440\u043e\u0446\u0435\u0441\u0456. \u041e\u043a\u0440\u0456\u043c \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u043d\u0438\u0445 \u0437\u0430\u043f\u0438\u0442\u0430\u043d\u044c, \u0434\u0456\u0437\u043d\u0430\u0439\u0442\u0435\u0441\u044f: \u0441\u043a\u0456\u043b\u044c\u043a\u0438 \u043c\u0456\u043d\u0456\u043c\u0443\u043c \u0440\u043e\u043a\u0456\u0432 \u0434\u043e\u0441\u0432\u0456\u0434\u0443 \u0437 \u043a\u043b\u044e\u0447\u043e\u0432\u0438\u043c\u0438 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0456\u044f\u043c\u0438 \u043f\u043e\u0442\u0440\u0456\u0431\u043d\u043e, \u044f\u043a\u0438\u0439 \u0441\u0442\u0435\u043a \u0456 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0456\u0457 \u043d\u0430\u0439\u0432\u0430\u0436\u043b\u0438\u0432\u0456\u0448\u0456, \u0430 \u0449\u043e nice-to-have. \u041e\u0441\u043e\u0431\u043b\u0438\u0432\u0443 \u0443\u0432\u0430\u0433\u0443 \u043f\u0440\u0438\u0434\u0456\u043b\u0456\u0442\u044c \u0442\u043e\u043c\u0443, \u0447\u043e\u043c\u0443 \u0432\u0456\u0434\u043a\u0440\u0438\u0442\u0430 \u0440\u043e\u043b\u044c, \u044f\u043a\u0438\u0439 \u0441\u043a\u043b\u0430\u0434 \u043a\u043e\u043c\u0430\u043d\u0434\u0438 \u0456 \u0441\u0442\u0430\u0434\u0456\u044f \u0440\u043e\u0437\u0440\u043e\u0431\u043a\u0438 (\u0443 \u0432\u0430\u0441 \u0432\u0441\u0435 \u043e\u0434\u043d\u043e \u043f\u0440\u043e \u0446\u0435 \u043f\u0435\u0440\u043