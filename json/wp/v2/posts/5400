u0442 \u0437\u0433\u0430\u0434\u0443\u0432\u0430\u0432 \u043d\u0430 \u0441\u043f\u0456\u0432\u0431\u0435\u0441\u0456\u0434\u0430\u0445. \u042f\u043a \u043c\u0438 \u0432\u0436\u0435 \u043a\u0430\u0437\u0430\u043b\u0438, \u044f\u043a\u0449\u043e \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0438 \u043f\u0440\u0430\u0446\u0456\u0432\u043d\u0438\u043a\u0430, \u044f\u043a\u0456 \u0432\u0456\u043d \u0442\u0430\u043a \u0434\u043e\u0432\u0433\u043e \u043d\u043e\u0441\u0438\u0432 \u0443 \u0441\u043e\u0431\u0456 \u0442\u0430 \u043f\u0440\u043e \u044f\u043a\u0456 \u043f\u0435\u0440\u0435\u0439\u043c\u0430\u0432\u0441\u044f, \u0442\u0430\u043a \u0448\u0432\u0438\u0434\u043a\u043e \u0432\u0438\u0440\u0456\u0448\u0438\u043b\u0438\u0441\u044f \u0442\u0456\u043b\u044c\u043a\u0438 \u043f\u0456\u0434 \u0437\u0430\u0433\u0440\u043e\u0437\u043e\u044e \u0437\u0432\u0456\u043b\u044c\u043d\u0435\u043d\u043d\u044f, \u0442\u043e \u0446\u0435 \u0433\u043e\u0432\u043e\u0440\u0438\u0442\u044c \u043f\u0440\u043e \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u044e \u0437 \u043d\u0435 \u043d\u0430\u0439\u043a\u0440\u0430\u0449\u043e\u0433\u043e \u0431\u043e\u043a\u0443. \u0420\u0430\u043f\u0442\u043e\u043c \u0432\u0430\u0448\u043e\u043c\u0443 \u043a\u0430\u043d\u0434\u0438\u0434\u0430\u0442\u0443 \u0437\u0430\u043f\u0440\u043e\u043f\u043e\u043d\u0443\u0432\u0430\u0442\u0438 \u043f\u0456\u0434\u0432\u0438\u0449\u0435\u043d\u043d\u044f \u0437\u0430\u0440\u043f\u043b\u0430\u0442\u0438, \u0430\u043b\u0435 \u043d\u0435 \u043f\u0440\u043e\u0441\u0443\u0432\u0430\u043d\u043d\u044f \u043a\u0430\u0440\u2019\u0454\u0440\u043d\u0438\u043c\u0438 \u0441\u0445\u043e\u0434\u0430\u043c\u0438, \u043d\u043e\u0432\u0456 \u0446\u0456\u043a\u0430\u0432\u0456 \u043f\u0440\u043e\u0454\u043a\u0442\u0438 \u0442\u0430 \u0448\u043e\u043a\u043e\u043b\u0430\u0434\u043d\u0435 \u043f\u0435\u0447\u0438\u0432\u043e \u0437\u0430\u043c\u0456\u0441\u0442\u044c \u043f\u0456\u0441\u043e\u0447\u043d\u043e\u0433\u043e, \u044f\u043a \u0432\u