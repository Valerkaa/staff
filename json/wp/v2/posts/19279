044c \u043a\u0440\u0438\u0442\u0435\u0440\u0456\u0457 \u043e\u0446\u0456\u043d\u043a\u0438 \u0434\u043e \u043a\u0430\u043d\u0434\u0438\u0434\u0430\u0442\u0430, \u0434\u0430\u0439\u0442\u0435 \u0440\u043e\u0437\u0433\u043e\u0440\u043d\u0443\u0442\u0438\u0439 \u0444\u0456\u0434\u0431\u0435\u043a. \u0412\u0438\u043a\u043e\u043d\u0430\u043d\u043d\u044f \u0437\u0430\u0432\u0434\u0430\u043d\u043d\u044f \u043c\u0430\u0454 \u0437\u0430\u0439\u043c\u0430\u0442\u0438 \u0434\u043e \u0434\u0432\u043e\u0445 \u0440\u043e\u0431\u043e\u0447\u0438\u0445 \u0434\u043d\u0456\u0432 \u0456 \u0431\u0443\u0442\u0438 \u043e\u043f\u043b\u0430\u0447\u0443\u0432\u0430\u043d\u0438\u043c (\u0431\u0430\u0436\u0430\u043d\u043e). \u0414\u0430\u0432\u0430\u0439\u0442\u0435 \u0430\u0440\u0433\u0443\u043c\u0435\u043d\u0442\u043e\u0432\u0430\u043d\u0438\u0439 \u0444\u0456\u0434\u0431\u0435\u043a \u0443\u0441\u0456\u043c \u043a\u0430\u043d\u0434\u0438\u0434\u0430\u0442\u0430\u043c.<\/li><li>\u0420\u0435\u043a\u0440\u0443\u0442\u0435\u0440 \u0447\u0438 HR \u043d\u0435 \u043c\u043e\u0436\u0435 \u043e\u0446\u0456\u043d\u044e\u0432\u0430\u0442\u0438 \u044f\u043a\u0456\u0441\u0442\u044c \u043f\u043e\u0440\u0442\u0444\u043e\u043b\u0456\u043e \u043a\u0430\u043d\u0434\u0438\u0434\u0430\u0442\u0430.<\/li><\/ul>\n\n\n\n<p>\u0417\u0430\u0433\u0430\u043b\u044c\u043d\u0456 \u043f\u043e\u0440\u0430\u0434\u0438 \u0437 \u0440\u0435\u043a\u0440\u0443\u0442\u0438\u043d\u0433\u0443 UI\/UX \u0434\u0438\u0437\u0430\u0439\u043d\u0435\u0440\u0456\u0432 \u043c\u0438 \u043e\u0431\u0433\u043e\u0432\u043e\u0440\u0438\u043b\u0438 \u0443 <a href=\"https:\/\/itexpert.work\/uk\/porady-z-rekrutyngu-ui-ux-dyzajneriv\/\" target=\"_blank\" rel=\"noopener\">\u0446\u0456\u0439 \u0441\u0442\u0430\u0442\u0442\u0456<\/a>. \u0421\u0432\u043e\u0457\u043c \u0434\u043e\u0441\u0432\u0456\u0434\u043e\u043c \u043d\u0430\u0439\u043c\u0443 \u0442\u0430\u043c \u043f\u043e\u0434\u0456\u043b\u0438\u043b\u0430\u0441\u044f \u041c\u0430\u0440\u0456\u044f \u