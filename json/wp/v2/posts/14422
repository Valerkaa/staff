430\u0439\u043c\u0443, \u0449\u043e\u043c\u0456\u0441\u044f\u0446\u044f \u0432\u0438\u043f\u043b\u0430\u0447\u0443\u0432\u0430\u0442\u0438 \u0439\u043e\u043c\u0443 \u0437\u0430\u0440\u043f\u043b\u0430\u0442\u043d\u044e, \u0447\u0435\u043a\u0430\u0442\u0438 \u0442\u0438\u0436\u043d\u0456, \u043f\u043e\u043a\u0438 \u0432\u0434\u0430\u0441\u0442\u044c\u0441\u044f \u00ab\u0437\u0430\u0445\u0430\u043d\u0442\u0438\u0442\u0438\u00bb \u043d\u043e\u0432\u0438\u0445 \u043a\u0430\u043d\u0434\u0438\u0434\u0430\u0442\u0456\u0432\u2026  \u0422\u043e\u043c\u0443 \u043d\u0430\u0439\u0447\u0430\u0441\u0442\u0456\u0448\u0435 \u0444\u0430\u0443\u043d\u0434\u0435\u0440\u0438 \u0441\u0442\u0430\u0440\u0442\u0430\u043f\u0456\u0432 \u0437\u0432\u0435\u0440\u0442\u0430\u044e\u0442\u044c\u0441\u044f \u0437\u0430 \u043f\u043e\u0441\u043b\u0443\u0433\u0430\u043c\u0438 \u0434\u043e \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u0439, \u044f\u043a\u0456 \u043f\u0456\u0434\u0431\u0438\u0440\u0430\u044e\u0442\u044c IT-\u0444\u0430\u0445\u0456\u0432\u0446\u0456\u0432 \u043d\u0430 \u0430\u0443\u0442\u0441\u043e\u0440\u0441\u0456 \u0447\u0438 \u0430\u0443\u0442\u0441\u0442\u0430\u0444\u0456. \u0422\u0430\u043a \u0432\u0434\u0430\u0454\u0442\u044c\u0441\u044f \u0448\u0432\u0438\u0434\u043a\u043e \u043f\u0456\u0434\u0442\u044f\u0433\u043d\u0443\u0442\u0438 \u0440\u043e\u0431\u043e\u0447\u0438\u0439 \u0440\u0435\u0441\u0443\u0440\u0441 \u043d\u0430 \u043f\u0440\u043e\u0454\u043a\u0442, \u0434\u043e\u043a\u0438 \u0456\u0434\u0435\u0457 \u043d\u0435 \u043f\u0435\u0440\u0435\u0433\u043e\u0440\u0456\u043b\u0438, \u0430 \u043a\u043e\u043d\u043a\u0443\u0440\u0435\u043d\u0442\u0438 \u043d\u0435 \u0432\u0438\u043f\u0443\u0441\u0442\u0438\u043b\u0438 \u0441\u0445\u043e\u0436\u0438\u0439 \u043f\u0440\u043e\u0434\u0443\u043a\u0442. \u0410\u043b\u0435 \u0439 \u0442\u0443\u0442 \u0454 \u043d\u044e\u0430\u043d\u0441\u0438.  \u042f\u043a\u0438\u0439 \u0432\u0430\u0440\u0456\u0430\u043d\u0442 \u0432\u0438\u0431\u0440\u04