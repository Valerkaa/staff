38 \u0432 \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u0457: \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0438 \u0437 \u043a\u043e\u043c\u0443\u043d\u0456\u043a\u0430\u0446\u0456\u0454\u044e; \u0441\u0443\u0446\u0456\u043b\u044c\u043d\u0438\u0439 \u043d\u0435\u0433\u0430\u0442\u0438\u0432 \u0442\u0430 \u043a\u0430\u0442\u0430\u0441\u0442\u0440\u043e\u0444\u0456\u0437\u043c \u043f\u0440\u043e\u0431\u043b\u0435\u043c; \u043d\u0435\u0432\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u043d\u0456\u0441\u0442\u044c \u043e\u0447\u0456\u043a\u0443\u0432\u0430\u043d\u043d\u044f \u0442\u0430 \u0440\u0435\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u0456 \u0443 \u043f\u043e\u0441\u0430\u0434\u043e\u0432\u0438\u0445 \u0456\u043d\u0441\u0442\u0440\u0443\u043a\u0446\u0456\u044f\u0445 \u0456 \u043f\u043e\u043b\u0456\u0442\u0438\u0446\u0456 \u043e\u0440\u0433\u0430\u043d\u0456\u0437\u0430\u0446\u0456\u0457; \u043f\u043e\u0433\u0430\u043d\u0435 \u0444\u0456\u0437\u0438\u0447\u043d\u0435 \u0442\u0430 \u0435\u043c\u043e\u0446\u0456\u0439\u043d\u0435 \u0437\u0434\u043e\u0440\u043e\u0432\u2019\u044f \u043f\u043e\u0442\u043e\u0447\u043d\u0438\u0445 \u0441\u043f\u0456\u0432\u0440\u043e\u0431\u0456\u0442\u043d\u0438\u043a\u0456\u0432.<\/p>\n\n\n\n<p>\u0429\u0435 \u043e\u0434\u0438\u043d \u0444\u0430\u043a\u0442\u043e\u0440, \u044f\u043a\u0438\u0439 \u0447\u0430\u0441\u0442\u043e \u0437\u0443\u0441\u0442\u0440\u0456\u0447\u0430\u0454\u0442\u044c\u0441\u044f \u0432 \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u044f\u0445 \u0456\u0437 \u0442\u043e\u043a\u0441\u0438\u0447\u043d\u043e\u044e \u0430\u0442\u043c\u043e\u0441\u0444\u0435\u0440\u043e\u044e, \u2014 \u0441\u043f\u0440\u043e\u0431\u0438 \u0434\u043e\u0432\u0435\u0441\u0442\u0438, \u0445\u0442\u043e \u0432 \u043a\u043e\u043c\u0430\u043d\u0434\u0456 \u0433\u043e\u043b\u043e\u0432\u043d\u0438\u0439, \u0437\u0430\u043c\u0456\u0441\u0442\u044c \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0438\u0432\u043d\u043e\u0457 \u0441\u043f\u0456\u043b\u044c\u043d\u043e\u0457 \u0440\u043e\u0431\u043e\u0442\u0438.<\/p>\n\n\n\n<p><\/p>\n\n\n\n<h3 class=\"wp-block-heading\">2. <strong>Cultural fit<\/strong><\/h3>\n\n\n\n<p><a href=\"https:\/\/itexpert.work\/ru\/polnaya-sinergiya-chto-vazhnee-v-komande-cultural-fit-ili-umenie-vypolnyat-zadachi\/\" target=\"_blank\" rel=\"noreferrer noopener\">Cultural fit<\/a> \u2014 \u0446\u0435 \u0432\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u043d\u0456\u0441\u0442\u044c \u043a\u043e\u0440\u043f\u043a\u0443\u043b\u044c\u0442\u0443\u0440\u0456 \u0442\u0430 \u0446\u0456\u043d\u043d\u043e\u0441\u0442\u044f\u043c. \u0423 \u043a\u043e\u0436\u043d\u0456\u0439 \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u0457 \u0454 \u0441\u0432\u043e\u0457 \u043c\u0430\u0440\u043a\u0435\u0440\u0438 \u043a\u043e\u043c\u043f\u0435\u0442\u0435\u043d\u0446\u0456\u0439 \u0442\u0430 \u0437\u0432\u0435\u0434\u0435\u043d\u043d\u044f \u043f\u0440\u0430\u0432\u0438\u043b. \u042f\u043a\u0449\u043e \u0432\u0438 \u0457\u043c \u043d\u0435 \u0432\u0456\u0434\u043f\u043e\u0432\u0456\u0434\u0430\u0454\u0442\u0435 \u0430\u0431\u043e \u0457\u0445 \u043d\u0435 \u043f\u0456\u0434\u0442\u0440\u0438\u043c\u0443\u0454\u0442\u0435, \u043f\u0440\u0430\u0446\u044e\u0432\u0430\u0442\u0438 \u0432 \u043a\u043e\u043c\u0430\u043d\u0434\u0456 \u0431\u0443\u0434\u0435 \u0441\u043a\u043b\u0430\u0434\u043d\u043e \u0442\u0430 \u043d\u0435\u043a\u043e\u043c\u0444\u043e\u0440\u0442\u043d\u043e. \u041d\u0430\u043f\u0440\u0438\u043a\u043b\u0430\u0434, \u0443 \u043f\u0440\u0438\u0441\u0443\u0442\u043d\u043e\u0441\u0442\u0456 \u0412\u043e\u043b\u0442\u0430 \u0414\u0456\u0441\u043d\u0435\u044f \u043d\u0435 \u043c\u043e\u0436\u043d\u0430 \u0431\u0443\u043b\u043e \u0432\u0438\u043c\u043e\u0432\u043b\u044f\u0442\u0438 \u0441\u043b\u043e\u0432\u0430 \u00abdamn\u00bb \u0430\u0431\u043e \u00abshit\u00bb \u2014 \u0443 \u043f\u0440\u043e\u0442\u0438\u043b\u0435\u0436\u043d\u043e\u043c\u0443 \u0432\u0438\u043f\u0430\u0434\u043a\u0443 \u0441\u043f\u0456\u0432\u0440\u043e\u0431\u0456\u0442\u043d\u0438\u043a\u0430 \u0437\u0432\u0456\u043b\u044c\u043d\u044f\u043b\u0438.<\/p>\n\n\n\n<p>\u0412\u0430\u0436\u043b\u0438\u0432\u043e, \u0449\u043e\u0431 \u043e\u0440\u0433\u0430\u043d\u0456\u0437\u0430\u0446\u0456\u044f \u0442\u0430\u043a\u043e\u0436 \u043c\u0430\u043b\u0430 \u0433\u043b\u043e\u0431\u0430\u043b\u044c\u043d\u0443 \u043c\u0456\u0441\u0456\u044e, \u0449\u043e \u0432\u0438\u0445\u043e\u0434\u0438\u0442\u044c \u0437\u0430 \u043c\u0435\u0436\u0456 \u043e\u0442\u0440\u0438\u043c\u0430\u043d\u043d\u044f \u043f\u0440\u0438\u0431\u0443\u0442\u043a\u0443. \u041d\u0430\u043f\u0440\u0438\u043a\u043b\u0430\u0434, \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u044f \u043f\u043e\u0441\u0442\u0430\u0432\u0438\u043b\u0430 \u0441\u043e\u0431\u0456 \u0437\u0430 \u043c\u0435\u0442\u0443 \u0440\u043e\u0437\u0440\u043e\u0431\u0438\u0442\u0438 \u0440\u043e\u0437\u0432\u2019\u044f\u0437\u0430\u043d\u043d\u044f \u043f\u0440\u043e\u0431\u043b\u0435\u043c\u0438 \u043c\u0456\u043b\u044c\u0439\u043e\u043d\u0456\u0432 \u043b\u044e\u0434\u0435\u0439. \u0422\u0430\u043a Uber \u0441\u0442\u0430\u0432 \u0430\u043b\u044c\u0442\u0435\u0440\u043d\u0430\u0442\u0438\u0432\u043e\u044e \u0443\u0431\u043e\u0433\u0456\u0439 \u0456\u043d\u0444\u0440\u0430\u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0456 \u0442\u0430\u043a\u0441\u0456, \u043d\u0438\u0437\u044c\u043a\u0456\u0439 \u044f\u043a\u043e\u0441\u0442\u0456 \u043e\u0431\u0441\u043b\u0443\u0433\u043e\u0432\u0443\u0432\u0430\u043d\u043d\u044f, \u0430 \u0442\u0430\u043a\u043e\u0436 \u0443\u0441\u0443\u043d\u0443\u0432 \u043c\u043e\u0432\u043d\u0438\u0439 \u0431\u0430\u0440\u2019\u0454\u0440, \u0430\u0434\u0436\u0435 \u043f\u043e\u0457\u0437\u0434\u043a\u0443 \u043c\u043e\u0436\u043d\u0430 \u043e\u0444\u043e\u0440\u043c\u0438\u0442\u0438 \u0447\u0435\u0440\u0435\u0437 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u0443 \u0442\u0430 \u0441\u043f\u043b\u0430\u0442\u0438\u0442\u0438 \u0457\u0457 \u043a\u0440\u0435\u0434\u0438\u0442\u043d\u043e\u044e \u043a\u0430\u0440\u0442\u043a\u043e\u044e.<\/p>\n\n\n\n<p><em>\u0426\u0456\u043d\u043d\u043e\u0441\u0442\u0456 ITExpert<\/em>:<\/p>\n\n\n<div class=\"lazyblock-slajder-ZwmcGo wp-block-lazyblock-slajder\"><div class=\"slider\">\r\n    <div>\r\n  <img width=\"1200\" height=\"800\" src=\"https:\/\/itexpert.work\/content\/uploads\/2022\/05\/korporatyvni-czinnosti-itexpert.jpg\" class=\"attachment-full size-full pp_post_image\" alt=\"\u041a\u043e\u0440\u043f\u043e\u0440\u0430\u0442\u0438\u0432\u043d\u0456 \u0446\u0456\u043d\u043d\u043e\u0441\u0442\u0456 ITExpert\" decoding=\"async\" loading=\"lazy\" title=\"\u0421\u0442\u0430\u0431\u0456\u043b\u044c\u043d\u0456\u0441\u0442\u044c \u0432\u0430\u0436\u043b\u0438\u0432\u0456\u0448\u0430 \u0437\u0430 \u0437\u0430\u0440\u043f\u043b\u0430\u0442\u043d\u044e: \u043a\u0440\u0438\u0442\u0435\u0440\u0456\u0457 \u0432\u0438\u0431\u043e\u0440\u0443 \u0440\u043e\u0431\u043e\u0442\u043e\u0434\u0430\u0432\u0446\u044f 2022 \u0440\u043e\u043a\u0443\" \/>  <\/div>\r\n  <\/div>\r\n<\/div>\n\n\n<p><\/p>\n\n\n\n<p><\/p>\n\n\n\n<h3 class=\"wp-block-heading\">3. <strong>\u041c\u043e\u0436\u043b\u0438\u0432\u043e\u0441\u0442\u0456 \u043e\u0441\u043e\u0431\u0438\u0441\u0442\u043e\u0433\u043e \u0442\u0430 \u043f\u0440\u043e\u0444\u0435\u0441\u0456\u0439\u043d\u043e\u0433\u043e \u0437\u0440\u043e\u0441\u0442\u0430\u043d\u043d\u044f<\/strong><\/h3>\n\n\n\n<p>\u0417\u0430 \u0434\u0430\u043d\u0438\u043c\u0438 <a href=\"https:\/\/thepoint.rabota.ua\/na-chto-ukrayntsy-obraschayut-vnymanye-vybyraya-rabotu-rezultaty-oprosa\/\" target=\"_blank\" rel=\"noopener nofollow\">\u043e\u043f\u0438\u0442\u0443\u0432\u0430\u043d\u043d\u044f<\/a> job-\u043f\u043b\u0430\u0442\u0444\u043e\u0440\u043c\u0438 Rabota.ua \u0437\u0430 2018 \u0440\u0456\u043a, \u043d\u0430 \u043f\u0435\u0440\u0448\u0438\u0445 \u043c\u0456\u0441\u0446\u044f\u0445 \u043f\u0456\u0434 \u0447\u0430\u0441 \u0432\u0438\u0431\u043e\u0440\u0443 \u0440\u043e\u0431\u043e\u0442\u043e\u0434\u0430\u0432\u0446\u044f \u0443 \u043a\u0430\u043d\u0434\u0438\u0434\u0430\u0442\u0456\u0432 \u2014 \u043f\u0440\u043e\u0444\u0435\u0441\u0456\u0439\u043d\u0456 \u043f\u0435\u0440\u0441\u043f\u0435\u043a\u0442\u0438\u0432\u0438. \u0410 \u0437\u0433\u0456\u0434\u043d\u043e \u0437 <a href=\"https:\/\/heartbeat.peakon.com\/reports\/the-9-month-warning-identifying-quitters-before-its-too-late\/\" target=\"_blank\" rel=\"noopener nofollow\" title=\"\u043e\u043f\u0438\u0442\u0443\u0432\u0430\u043d\u043d\u044f\u043c\">\u043e\u043f\u0438\u0442\u0443\u0432\u0430\u043d\u043d\u044f\u043c<\/a> Peakon 32 \u043c\u043b\u043d \u043d\u0430\u0439\u043c\u0430\u043d\u0438\u0445 \u0444\u0430\u0445\u0456\u0432\u0446\u0456\u0432 \u0456\u0437 125 \u043a\u0440\u0430\u0457\u043d \u043c\u043e\u0436\u043d\u0430 \u0432\u0438\u044f\u0432\u0438\u0442\u0438 \u043e\u0437\u043d\u0430\u043a\u0438, \u044f\u043a\u0456 \u0449\u0435 \u043d\u0430 \u0440\u0430\u043d\u043d\u0456\u0445 \u0435\u0442\u0430\u043f\u0430\u0445 \u0432\u043a\u0430\u0437\u0443\u044e\u0442\u044c \u043d\u0430 \u0440\u0438\u0437\u0438\u043a \u0432\u0442\u0440\u0430\u0442\u0438 \u0441\u043f\u0456\u0432\u0440\u043e\u0431\u0456\u0442\u043d\u0438\u043a\u0430.<\/p>\n\n\n\n<p>\u0412\u0438\u044f\u0432\u0438\u043b\u043e\u0441\u044f, \u0449\u043e \u0444\u0430\u0445\u0456\u0432\u0446\u0456 \u0447\u0430\u0441\u0442\u043e \u0439\u0434\u0443\u0442\u044c \u0456\u0437 \u043a\u043e\u043c\u043f\u0430\u043d\u0456\u0457, \u0442\u043e\u043c\u0443 \u0449\u043e \u0457\u0445\u043d\u044f \u0440\u043e\u0431\u043e\u0442\u0430 \u0434\u0443\u0436\u0435 \u043e\u0434\u043d\u043e\u043c\u0430\u043d\u0456\u0442\u043d\u0430 \u0439 \u043f\u0440\u043e\u0441\u0442\u0430. \u0421\u043a\u043b\u0430\u0434\u043d\u0456, \u0430\u043b\u0435 \u0446\u0456\u043a\u0430\u0432\u0456 \u0442\u0430\u0441\u043a\u0438 \u0437\u0430\u043f\u043e\u0431\u0456\u0433\u0430\u044e\u0442\u044c \u043f\u0440\u043e\u0444\u0435\u0441\u0456\u0439\u043d\u043e\u043c\u0443 \u0432\u0438\u0433\